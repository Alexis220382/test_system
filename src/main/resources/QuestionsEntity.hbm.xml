<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ivanovskiy.entity.QuestionsEntity" table="questions" schema="" catalog="test_db">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="1000"/>
        </property>
        <property name="first">
            <column name="first" sql-type="varchar" length="1000"/>
        </property>
        <property name="second">
            <column name="second" sql-type="varchar" length="1000"/>
        </property>
        <property name="third">
            <column name="third" sql-type="varchar" length="1000"/>
        </property>
        <set name="tests" cascade="save-update" table="testquestions">
            <key column="id_questions"/>
            <many-to-many column="id_tests" fetch="join" class="com.ivanovskiy.entity.TestsEntity"/>
        </set>
    </class>
</hibernate-mapping>