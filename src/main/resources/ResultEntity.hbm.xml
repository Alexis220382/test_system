<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ivanovskiy.entity.ResultEntity" table="result">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="id_questions">
            <column name="id_questions" sql-type="int" not-null="true"/>
        </property>
        <property name="login">
            <column name="login" sql-type="varchar" length="55"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" length="55"/>
        </property>
    </class>
</hibernate-mapping>