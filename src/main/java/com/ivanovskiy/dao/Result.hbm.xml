<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ivanovskiy.dao.Result" table="result" schema="" catalog="test_db">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="idTest">
            <column name="id_test" sql-type="int"/>
        </property>
        <property name="idQuestion">
            <column name="id_question" sql-type="int"/>
        </property>
        <property name="login">
            <column name="login" sql-type="varchar" length="55"/>
        </property>
        <property name="res">
            <column name="res" sql-type="int"/>
        </property>
        <property name="isRight">
            <column name="is_right" sql-type="int"/>
        </property>
        <property name="datePass">
            <column name="date_pass" sql-type="varchar" length="25"/>
        </property>
    </class>
</hibernate-mapping>